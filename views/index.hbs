<section id="heroes">
    <!-- this is a loop with handlebars -->
    {{#each carInfo}}
        <div class="hero-container">
            <a href="/{{car_name}}">
                <img src="/images/{{car_img}}" alt="avatar">
            </a>

            <h3 class="hero-name">{{car_name}}</h3>
        </div>
    {{/each}}
</section>
